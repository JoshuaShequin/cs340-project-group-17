<script src="/index.js" charset="utf-8" defer></script>

<div class="page-title">
	<span>Home Page</span>
</div>

<div class="page-content">

	<div class="recent-tests-container">
		<span class="top-of-container-text">Recent Tests</span>
		<section id="find-tests-results">
		{{#each recentTests}}
			{{> findTestsInfoTemplate}}
		{{/each}}
	</section>
	</div>
	
	<div class="popular-tests-container">
		<span class="top-of-container-text">Popular Tests</span>
		<section id="find-tests-results">
		{{#each popularTests}}
			{{> findTestsInfoTemplate}}
		{{/each}}
	</section>
	</div>
	
	<div class="your-tests-container">
		<span class="top-of-container-text">Your Tests</span>
		<section id="find-tests-results">
		{{#each yourTests}}
			{{> YourTestsTemplate}}
		{{/each}}
	</section>
	</div>
	
	<div class="logout-container">
		<button type="submit" id="logout-button" value="">Logout</button>
	</div>

</div>

